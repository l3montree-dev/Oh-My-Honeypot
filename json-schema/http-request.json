{
    "$schema": "https://json-schema.org/draft/2019-09/schema",
    "$id": "https://github.com/l3montree-dev/oh-my-honeypot/json-schema/http-request.json",
    "type": "object",
    "default": {},
    "title": "Root Schema",
    "required": [
        "port",
        "accept-language",
        "user-agent"
    ],
    "properties": {
        "port": {
            "type": "string",
            "default": "",
            "title": "The port which got scanned",
            "examples": [
                "8080"
            ]
        },
        "path": {
            "type": "string",
            "default": "",
            "title": "URL path",
            "examples": [
                "/index.html"
            ]
        },
        "method": {
            "type": "string",
            "default": "",
            "title": "The HTTP method",
            "examples": [
                "GET"
            ]
        },
        "acpt-language": {
            "type": "string",
            "default": "",
            "title": "The accept language",
            "examples": [
                "en-US,en;q=0.5"
            ]
        },
        "user-agent": {
            "type": "string",
            "default": "",
            "title": "The user agent",
            "examples": [
                "Mozilla/5.0 (compatible; MSIE 10.0; Windows NT 6.1; Trident/6.0)"
            ]
        },
        "content-type": {
            "type": "string",
            "default": "",
            "title": "The content type",
            "examples": [
                "text/html"
            ]
        }
    }
}