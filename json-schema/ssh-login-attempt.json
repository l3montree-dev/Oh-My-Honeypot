{
    "$schema": "https://json-schema.org/draft/2019-09/schema",
    "$id": "https://gitlab.com/neuland-homeland/honeypot/json-schema/ssh-login-attempt.json",
    "type": "object",
    "default": {},
    "title": "Root Schema",
    "required": [
        "username",
        "password",
        "ipAddress"
    ],
    "properties": {
        "username": {
            "type": "string",
            "default": "",
            "title": "The username Schema",
            "examples": [
                "root"
            ]
        },
        "password": {
            "type": "string",
            "default": "",
            "title": "The password Schema",
            "examples": [
                "root-pw"
            ]
        },
        "ipAddress": {
            "type": "string",
            "default": "",
            "title": "The ipAddress Schema",
            "examples": [
                "127.0.0.1"
            ]
        }
    },
    "examples": [
        {
            "username": "root",
            "password": "root-pw",
            "ipAddress": "127.0.0.1"
        }
    ]
}