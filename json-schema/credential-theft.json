{
    "$schema": "https://json-schema.org/draft/2019-09/schema",
    "$id": "https://github.com/l3montree-dev/oh-my-honeypot/json-schema/credential_theft.json",
    "type": "object",
    "default": {},
    "title": "Root Schema",
    "required": [
        "ssh_pw",
        "db_pw"
    ],
    "properties": {
        "ssh_pw": {
            "type": "string",
            "default": "",
            "title": "The password Schema",
            "examples": [
                "123456"
            ]
        },
        "db_pw": {
            "type": "string",
            "default": "",
            "title": "The password Schema",
            "examples": [
                "123456"
            ]
        }
    },
    "examples": [
        {
            "ssh_pw": "1234567",
            "db_pw": "123456"
        }
    ]
}